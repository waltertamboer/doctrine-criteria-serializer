{
    "name": "waltertamboer/doctrine-criteria-serializer",
    "description": "A small PHP library that can be used to serialize a Doctrine Criteria object.",
    "keywords": ["doctrine", "criteria", "serialize", "unserialize", "serializer"],
    "homepage": "https://github.com/waltertamboer/doctrine-criteria-serializer",
    "license": "MIT",
    "authors": [
        {
            "name": "Walter Tamboer",
            "email": "walter@tamboer.nl",
            "homepage": "http://www.waltertamboer.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/collections": "v1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "fabpot/php-cs-fixer": "v1.1"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Common\\Collections\\": "src/"
        }
    }
}
